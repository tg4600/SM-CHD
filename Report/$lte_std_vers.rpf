ROUTINE validation_routine

DECLARE ok, lifetime, the_type

    { Called on pressing the DO key to save LTE screen.               }
    { Called outside of the LTE write transaction.                    }
        {                                                                 }
    { Returns a boolean value to indicate whether to proceed with     }
    { the save process - if FALSE is returned the user is forced back }
    { into the editor                                                 }

    ok = TRUE

    lifetime = SELECT standard_versions . lifetime
    the_type = SELECT standard_versions . expiry_type

    IF ( ( the_type = TIME_TYPE ) OR
         ( the_type = BOTH_TYPE ) ) AND
       ( lifetime <> ZERO_INT     ) THEN

        IF ( lifetime > ZERO_INT ) AND
           ( lifetime < INTERVAL ( standard_minimum_lifetime ) ) THEN

            flash_message ( GET_USER_MESSAGE ( "LTE_STDS_MESS_1" , 1 ) :
                            standard_minimum_lifetime , true )

            ok = FALSE
        ELSE
            ASSIGN standard_versions . expiry_date =
                        ( SELECT standard_versions . creation_date ) + lifetime

        ENDIF
    ELSE
        ASSIGN standard_versions . expiry_date = date ( "" )
    ENDIF

    RETURN ( ok )

ENDROUTINE

