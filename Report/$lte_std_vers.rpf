ROUTINE standard_version_lifetime ( VALUE column     ,
                                    VALUE row        ,
                                    VALUE cell_width ,
                                          window     ,
                                    VALUE is_display )

{
*   Prompt for replicate number. If a library and routine have been specified
*   prompt is not applicable.
*
******************************************************************************}

    DECLARE lifetime, form, labtable

    lifetime = SELECT standard_versions . lifetime

    IF is_display THEN

        DISPLAY lifetime
            ON LINE   row
            FROM      column
            TO        column + cell_width - 1
            IN WINDOW window

    ELSE

        PROMPT  FOR       lifetime
            ON LINE   row
            FROM      column
            TO        column + cell_width - 1
            IN WINDOW window
            FORMAT standard_versions . lifetime

        ASSIGN standard_versions . lifetime = lifetime

        IF lifetime = ZERO_INT THEN

            ASSIGN standard_versions . expiry_date = DATE ( "" )
        ELSE
            ASSIGN standard_versions . expiry_date =
                    ( SELECT standard_versions . creation_date ) + lifetime

        ENDIF

        labtablib_get_form ( form )
        labtablib_get_labtable ( labtable )

        form . prompt_objects [ labtable . editor_prompt ] . redisplay_contents ( )


    ENDIF

ENDROUTINE

