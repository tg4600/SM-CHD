ROUTINE read_data_routine (       labtable  ,
                            VALUE operation )

DECLARE vers_ident , version_num

    { Called after the record has been selected / reserved but before }
    { the editor screen has been built.                               }
        {                                                                 }
    { labtable      the labtable object.                              }
    { operation     the current LTE operation eg ADD, MODIFY, DISPLAY }
        {                                                                 }
    { No return value is required                                     }


    IF ( operation = "NEW_VERSION" ) THEN

        vers_ident  = SUBSTRING ( labtable . identity , 1 , 10  )
        version_num = SUBSTRING ( labtable . identity , 11 , 10 )

        std_vers_reserve_new_standard_version ( vers_ident  ,
                                version_num )
    ENDIF


